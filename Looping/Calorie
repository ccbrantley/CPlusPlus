//Calculates calories burned
// 3.9 constant per five minutes
#include <iostream>
#include <iomanip>
using namespace std;

int main() {
	cout << setw(10) << left << "Minutes" << right << "Calories" << endl;
	for (int i = 5; i <= 30; i += 5) {
		int caloriesBurned = 3.9 * i;
		cout << setw(10) << left << i << right << caloriesBurned << endl;
	}
	system("pause");
	return 0;
}
