// Creates a table of conversion for
// Fahrenheit and Celsius from 0-32
#include <iostream>
#include <iomanip>
using namespace std;

int main() {
	double fahrenheit;
	cout << setw(15) << left << "Fahrenheit" << right << "Celsius" << endl;
	cout << setprecision(1) << showpoint << fixed;
	for (int i = 0; i <= 32; i++) {
		fahrenheit = (9.00 / 5) * i + 32;
		cout << setw(15) << left << fahrenheit << right << i << endl;
	}
	
	system("pause");
	return 0;
}
