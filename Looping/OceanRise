// Table of Increase
// Ocean Levels at 3.1 mm per year
#include <iostream>
#include <iomanip>
using namespace std;

int main() {
	cout << setw(19) << left << "Ocean Level(mm)" << right << "year" << endl;
	cout << fixed << showpoint << setprecision(2);
	for (int i = 1; i <= 25; i++) {
		double oceanLevel = i * 3.1;
		cout << setw(19) << left << oceanLevel << right << i << endl;
	}

	system("pause");
	return 0;
}
